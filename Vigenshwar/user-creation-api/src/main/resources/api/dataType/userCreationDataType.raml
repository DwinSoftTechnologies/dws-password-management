#%RAML 1.0 DataType
        type: object
        properties:
          "firstName":
            type: string
            required: true
          "lastName":
            type: string
            required: false
          "userName":
            type: string
            required: true 
          "email":
            type: string
            pattern: "^[a-z0-9._-]+@[a-z0-9.-]+[a-z]{2,}$"
            required: true
          "company":
            type: string
            enum:
              - Juul
            required: true
          "phoneNumber":
            type: string
            pattern: "^(?:[0-9]{10}|\\+?[0-9]{1,3}[- ]?[0-9]{10})$"
            required: true
          "password":
            type: string
            maxLength: 10
            minLength: 5
            required: true