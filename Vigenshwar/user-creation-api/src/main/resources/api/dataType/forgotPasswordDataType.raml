#%RAML 1.0 DataType
        type: object
        properties:
          "email":
            type: string
            pattern: "^[a-z0-9._-]+@[a-z0-9.-]+[a-z]{2,}$"
            required: true
          "password":
            type: string
            maxLength: 10
            minLength: 5
            required: true